sound.Looped = false
sound:Play()
task.wait(0.5)
  partTweenn1 = game:GetService("TweenService"):Create(tool, TweenInfo.new(0.4, Enum.EasingStyle.Linear), {
        Grip = CFrame.Angles(math.rad(90), 0, 0),
        GripPos = Vector3.new(0, -1.6, -0.5)
    })
    partTweenn1:Play()
